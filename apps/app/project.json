{
  "name": "app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/app/src",
  "targets": {
    "dev": {
      "inputs": [
        {
          "externalDependencies": ["vite"]
        }
      ],
      "command": "cd apps/app && vite",
      "dependsOn": ["^build"]
    },
    "build": {
      "inputs": [
        {
          "externalDependencies": ["vite", "vue-tsc"]
        }
      ],
      "command": "cd apps/app && vue-tsc && vite build",
      "dependsOn": ["^build"]
    },
    "preview": {
      "inputs": [
        {
          "externalDependencies": ["vite"]
        }
      ],
      "command": "cd apps/app && vite preview"
    }
  }
}
