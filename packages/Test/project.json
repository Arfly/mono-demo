{
  "name": "Test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/Test/src",
  "targets": {
    "dev": {
      "inputs": [
        {
          "externalDependencies": ["vite"]
        }
      ],
      "command": "cd packages/Test && vite",
      "dependsOn": ["^build"]
    },
    "build": {
      "inputs": [
        {
          "externalDependencies": ["vite", "vue-tsc"]
        }
      ],
      "command": "cd packages/Test && vue-tsc && vite build",
      "dependsOn": ["^build"]
    },
    "preview": {
      "inputs": [
        {
          "externalDependencies": ["vite"]
        }
      ],
      "command": "cd packages/Test && vite preview"
    }
  }
}
